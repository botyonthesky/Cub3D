{
	"configurations": [
	{
		"name": "(gdb) cub3d",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceRoot}/cub3D",
		"args": [
			"maps/bonus/little_map.cub"
		],
		"stopAtEntry": false,
		"cwd": "${workspaceRoot}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			}
		],
		"preLaunchTask": "cub3d"
	}
	]
}